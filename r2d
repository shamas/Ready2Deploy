    echo $1
}

for f in `cat $inputfile`
do
    bf=`basename $f`
    echo $f": "$bf
    if [ -e $f ]
    then
    echo cp $f $out$bf
    cp $f $out$bf
    else
    echo NE
    fi
done

replace there

echo "FINISHED"
ls ./live
